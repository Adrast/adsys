<policyDefinitions xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" revision="1.0" schemaVersion="1.0" xmlns="http://schemas.microsoft.com/GroupPolicy/2006/07/PolicyDefinitions">
  <policyNamespaces>
    <target prefix="desktop" namespace="Microsoft.Policies.WindowsDesktop" />
    <using prefix="windows" namespace="Microsoft.Policies.Windows" />
  </policyNamespaces>
  <resources minRequiredRevision="1.0" />

  <categories>
    <category name="UbuntuCategory1DisplayName" displayName="$(string.UbuntuDisplayCategory1DisplayName)">
      <parentCategory ref="windows:Desktop" />
    </category>
  </categories>

  <policies>
    <policy name="UbuntuSummary" class="Machine" displayName="$(string.UbuntuDisplaySummary)" explainText="$(string.UbuntuExplainTextSummary)" presentation="$(presentation.UbuntuPresentationSummary)" key="Software\Ubuntu\dconf\org\gnome\desktop\policy-choices" valueName="metaValues">
      <parentCategory ref="UbuntuCategory1DisplayName" />
      <supportedOn ref="Ubuntu" />
      <enabledValue><string>"all": {"meta": "s", "default": "''"}</string></enabledValue>
      <elements>
          <enum id="UbuntuElemSummary" valueName="all">
              <item displayName="$(string.UbuntuItemSummary0)">
                <value>
                    <string>Choice 1</string>
                </value>
              </item>
              <item displayName="$(string.UbuntuItemSummary1)">
                <value>
                    <string>Choice 2</string>
                </value>
              </item>
              <item displayName="$(string.UbuntuItemSummary2)">
                <value>
                    <string>Choice 3</string>
                </value>
              </item>
              <item displayName="$(string.UbuntuItemSummary3)">
                <value>
                    <string>Choice 4</string>
                </value>
              </item>
          </enum>
      </elements>
    </policy>
  </policies>

</policyDefinitions>