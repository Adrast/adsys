<policyDefinitions xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" revision="1.0" schemaVersion="1.0" xmlns="http://schemas.microsoft.com/GroupPolicy/2006/07/PolicyDefinitions">
  <policyNamespaces>
    <target prefix="desktop" namespace="Microsoft.Policies.WindowsDesktop" />
    <using prefix="windows" namespace="Microsoft.Policies.Windows" />
  </policyNamespaces>
  <resources minRequiredRevision="1.0" />

  <categories>
    <category name="UbuntuCategory1DisplayName" displayName="$(string.UbuntuDisplayCategory1DisplayName)">
      <parentCategory ref="windows:Desktop" />
    </category>
    <category name="UbuntuCategory2DisplayName" displayName="$(string.UbuntuDisplayCategory2DisplayName)">
      <parentCategory ref="windows:Desktop" />
    </category>
  </categories>

  <policies>
    <policy name="UbuntuSummary1" class="Machine" displayName="$(string.UbuntuDisplaySummary1)" explainText="$(string.UbuntuExplainTextSummary1)" presentation="$(presentation.UbuntuPresentationSummary1)" key="Software\Ubuntu\dconf\org\gnome\desktop\policy-multiple1" valueName="metaValues">
      <parentCategory ref="UbuntuCategory1DisplayName" />
      <enabledValue><string>"all": {"meta": "s", "default": "''"}</string></enabledValue>
      <elements>
        <text id="UbuntuElemSummary1" valueName="all" />
      </elements>
    </policy>
    <policy name="UbuntuSummary2" class="Machine" displayName="$(string.UbuntuDisplaySummary2)" explainText="$(string.UbuntuExplainTextSummary2)" presentation="$(presentation.UbuntuPresentationSummary2)" key="Software\Ubuntu\dconf\org\gnome\desktop\policy-multiple2" valueName="metaValues">
      <parentCategory ref="UbuntuCategory2DisplayName" />
      <enabledValue><string>"all": {"meta": "s", "default": "''"}</string></enabledValue>
      <elements>
        <text id="UbuntuElemSummary2" valueName="all" />
      </elements>
    </policy>
  </policies>

</policyDefinitions>